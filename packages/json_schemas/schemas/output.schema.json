{
    "$id": "https://apify.com/schemas/v1/output.json",
    "title": "JSON schema of Apify Actor output schema",
    "type": "object",
    "properties": {
        "$schema": {
            "type": "string"
        },
        "actorOutputSchemaVersion": {
            "type": "integer",
            "minimum": 1,
            "maximum": 1
        },
        "description": {  "type": "string" },
        "title": { "type": "string" },
        "properties": {
            "type": "object",
            "patternProperties": {
                "^": {
                    "title": "Output schema property",
                    "type": "object",
                    "additionalProperties": false,
                    "properties": {
                        "title": { "type": "string" },
                        "type": { "const": "string" },
                        "description": { "type": "string" },
                        "template": { "type": "string" },
                        "autoRefresh": { "type": "boolean" }
                    },
                    "required": [
                        "type",
                        "template"
                    ]
                }
            }
        }
    },
    "required": [
        "actorOutputSchemaVersion",
        "properties"
    ]
}

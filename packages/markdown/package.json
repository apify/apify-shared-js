{
    "name": "@apify/markdown",
    "version": "2.0.6",
    "description": "Tools and constants shared across Apify projects.",
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "typings": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.mjs",
